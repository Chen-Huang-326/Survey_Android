<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape
            android:shape="oval"
            android:useLevel="true">
            <size
                android:width="80dp"
                android:height="80dp" />

            <solid android:color="#FDFDFD" />
            <stroke
                android:width="3dp"
                android:color="#F49255" />
        </shape>
    </item>

    <item
        android:bottom="10dp"
        android:left="10dp"
        android:right="10dp"
        android:top="10dp">
        <shape
            android:shape="oval"
            android:useLevel="true">
            <size
                android:width="80dp"
                android:height="80dp" />
            <solid android:color="#FEE46B" />
        </shape>
    </item>

</layer-list>